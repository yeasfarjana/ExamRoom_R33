@model ExamRoom_R33.Models.TestXPaper

@{
    ViewBag.Title = "Index";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/MyStyleSheet.css" rel="stylesheet" />



<div id="boxResult">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div class="panel panel-default" id="panelRes" style="margin-top:120px;">
                    <div class="panel-body">
                            <div class="panel-heading">
                                <h1 class="text-center" style="margin:0;padding:0;">Your Score is <span style="font-size:40px;color:green">@ViewBag.ExamineeMarks</span>  Out of <span style="font-size:40px;color:green">@ViewBag.totalMarksOfAllQ</span></h1>
                                <hr />
                            </div>
                            <div class="text-left" style="margin-left:60px;">
                                <h4>Total Question: &nbsp; <span>@ViewBag.TotalQuestion</span></h4><br />
                                <h4>Total Marks: &nbsp; <span>@ViewBag.totalMarksOfAllQ</span> </h4><br />
                                <h4>Passing Marks: &nbsp; <span>@ViewBag.PassingMarks</span> </h4><br />
                                <h4>Obtain Marks: &nbsp; <span>@ViewBag.ExamineeMarks</span> </h4><br />
                                <h4>Status:&nbsp;&nbsp;&nbsp;<span style="font-size:30px;color:red">@ViewBag.Status</span></h4><br />
                            </div>
                        <div class="col-md-offset-10 col-md-2">
                            <a class="btn btn-primary" href="@Url.Action("Print", "TestXPaper")" target="_blank">&nbsp;Print</a>
                        </div>
                        
                    </div>
                </div>                
            </div>
        </div>
    </div>
</div>

